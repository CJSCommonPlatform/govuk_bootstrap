<!DOCTYPE html>
<html lang="en" data-ng-app="app" id="ng-app">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta id="viewport" name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
  <link rel="shortcut icon" href="assets/images/favicon.ico" type="image/x-icon" />
  <link rel="mask-icon" href="assets/images/gov.uk_logotype_crown.svg" color="#0b0c0c">
  <title>GovUK Bootstrap</title>
  <link rel="stylesheet" href="assets/govuk_bootstrap.css" type="text/css"/>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/prism/1.4.1/themes/prism.min.css"/>
</head>
<script>
  document.addEventListener("DOMContentLoaded", init, false);
  function init() {
    var codeBlocks = document.querySelectorAll("pre code");
    if(codeBlocks.length) {
      for(var i=0, len=codeBlocks.length; i<len; i++) {
        var dom = codeBlocks[i];
        var html = dom.innerHTML;
        html = html.replace(/</g,"&lt;").replace(/>/g,"&gt;");
        dom.innerHTML = html;
      }
    }
  }
</script>
<body id="top">
  {{> header}}
  <div id="global-header-bar"></div>
  <main role="main">
    <div class="container">
      <h1>{{_page.title}}</h1>
      {{{ content }}}
    </div>
  </main>
  {{> footer}}
  <script src="https://cdnjs.cloudflare.com/ajax/libs/prism/1.4.1/components/prism-core.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/prism/1.4.1/components/prism-markup.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/prism/1.4.1/components/prism-clike.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/prism/1.4.1/components/prism-javascript.min.js"></script>

</body>
</html>
